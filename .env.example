# ============================================================================
# 应用配置
# ============================================================================

# 应用环境 (development | production | test)
VITE_APP_ENV=development

# 应用名称
VITE_APP_NAME=山西电网智能报告系统

# 应用版本
VITE_APP_VERSION=1.0.0

# API 基础URL
VITE_API_BASE_URL=http://localhost:3000/api

# 文件上传限制 (MB)
VITE_MAX_FILE_SIZE=100

# 支持的文件类型
VITE_SUPPORTED_FILE_TYPES=.xlsx,.xls,.csv,.json,.db

# ============================================================================
# AI 服务配置
# ============================================================================

# 默认 AI 提供商
VITE_DEFAULT_AI_PROVIDER=qwen

# AI 请求超时时间 (毫秒)
VITE_AI_REQUEST_TIMEOUT=30000

# AI 服务重试次数
VITE_AI_MAX_RETRIES=3

# 启用 AI 服务缓存
VITE_AI_CACHE_ENABLED=true

# AI 缓存过期时间 (秒)
VITE_AI_CACHE_TTL=3600

# ============================================================================
# 分析配置
# ============================================================================

# 最大并发分析任务数
VITE_MAX_CONCURRENT_ANALYSIS=5

# 分析任务超时时间 (秒)
VITE_ANALYSIS_TIMEOUT=600

# 启用分析结果缓存
VITE_ANALYSIS_CACHE_ENABLED=true

# ============================================================================
# 报告配置
# ============================================================================

# 报告自动保存间隔 (秒)
VITE_REPORT_AUTO_SAVE_INTERVAL=30

# 报告历史版本保留数量
VITE_REPORT_VERSION_LIMIT=10

# 启用报告协作编辑
VITE_REPORT_COLLABORATION_ENABLED=true

# ============================================================================
# 监控和日志配置
# ============================================================================

# 启用性能监控
VITE_MONITORING_ENABLED=true

# 日志级别 (debug | info | warn | error)
VITE_LOG_LEVEL=info

# 启用错误上报
VITE_ERROR_REPORTING_ENABLED=true

# 错误上报服务URL
VITE_ERROR_REPORTING_URL=https://your-error-reporting-service.com

# ============================================================================
# 安全配置
# ============================================================================

# JWT 密钥 (生产环境请使用强密钥)
VITE_JWT_SECRET=your_jwt_secret_key_here

# 会话超时时间 (小时)
VITE_SESSION_TIMEOUT=24

# 启用 HTTPS (生产环境建议启用)
VITE_HTTPS_ENABLED=false

# CORS 允许的域名
VITE_CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# ============================================================================
# 数据库配置 (如果使用自定义数据库)
# ============================================================================

# 数据库连接URL
DATABASE_URL=postgresql://username:password@localhost:5432/smart_report

# 数据库连接池大小
DATABASE_POOL_SIZE=10

# 数据库连接超时 (毫秒)
DATABASE_TIMEOUT=5000

# ============================================================================
# Redis 配置 (如果使用 Redis 缓存)
# ============================================================================

# Redis 连接URL
REDIS_URL=redis://localhost:6379

# Redis 密码
REDIS_PASSWORD=your_redis_password

# Redis 数据库索引
REDIS_DB=0

# ============================================================================
# 文件存储配置
# ============================================================================

# 文件存储类型 (local | supabase | s3)
VITE_STORAGE_TYPE=supabase

# 本地存储路径
VITE_LOCAL_STORAGE_PATH=./uploads

# AWS S3 配置 (如果使用 S3)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-s3-bucket-name

# ============================================================================
# 邮件服务配置 (如果需要邮件通知)
# ============================================================================

# SMTP 服务器
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_email_password
SMTP_FROM=noreply@your-domain.com

# ============================================================================
# 第三方服务配置
# ============================================================================

# Google Analytics ID
VITE_GA_ID=G-XXXXXXXXXX

# Sentry DSN (错误监控)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# 百度统计 ID
VITE_BAIDU_ANALYTICS_ID=your_baidu_analytics_id

# ============================================================================
# 开发环境配置
# ============================================================================

# 开发服务器端口
VITE_DEV_PORT=5173

# 开发服务器主机
VITE_DEV_HOST=localhost

# 启用热重载
VITE_HMR_ENABLED=true

# 启用开发工具
VITE_DEV_TOOLS_ENABLED=true

# Mock 数据模式
VITE_MOCK_MODE=false

# ============================================================================
# 生产环境配置
# ============================================================================

# 生产环境域名
VITE_PRODUCTION_DOMAIN=https://your-production-domain.com

# CDN 基础URL
VITE_CDN_BASE_URL=https://your-cdn-domain.com

# 启用 PWA
VITE_PWA_ENABLED=true

# 启用 Gzip 压缩
VITE_GZIP_ENABLED=true

# 构建输出目录
VITE_BUILD_OUTPUT_DIR=dist

# ============================================================================
# 注意事项
# ============================================================================
# 1. 请将此文件复制为 .env 并填入实际的配置值
# 2. 不要将包含真实密钥的 .env 文件提交到版本控制系统
# 3. 生产环境请使用强密码和安全的密钥
# 4. 定期轮换 API 密钥以确保安全性
# 5. 使用环境变量管理工具（如 dotenv）来管理不同环境的配置